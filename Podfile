	pod 'NVActivityIndicatorView'
	pod 'CocoaLumberjack/Swift'
        pod 'Appirater'
	pod 'OneSignal'
	pod 'Hero'
  # Pods for LS

  target 'LSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
